{
  "expo": {
    "name": "Expiration",
    "slug": "Expiration",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/appstore.png",
    "splash": {
      "image": "./assets/icon/appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "http://192.168.1.3:3000/graphql": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            }
          }
        },
        "NSCameraUsageDescription": "유통기한 관리사에서 카메라를 사용합니다.",
        "NSMicrophoneUsageDescription": "유통기한 관리사에서 마이크를 사용합니다.",
        "NSPhotoLibraryAddUsageDescription": "유통기한 관리사에서 앨범을 사용합니다.",
        "NSPhotoLibraryUsageDescription": "유통기한 관리사에서 앨범을 사용합니다.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "bundleIdentifier": "com.luckycontrol.Expiration"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
